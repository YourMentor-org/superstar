.btn
    display inline-block
    border none
    text-transform uppercase
    font-size .68em
    border-radius 3.63em
    padding 1.36em 3.63em
    background-color rgba(255, 255, 255, .2)
    color #fff
    &:hover, &:focus
        background-color rgba(255, 255, 255, .3)
    & + &
        margin-left .93em
